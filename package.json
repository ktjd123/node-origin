{
  "scripts": {
    "d": "nodemon -w package.json -w src --exec babel-node src/index.js",
    "build": "rm -rf build && babel src -d build && cp package.json ./build",
    "p": "pkg build/index.js --out-path build/executable",
    "start": "node ./build/index.js",
    "pm2": "pm2 start build/index.js -i 0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "colors": "^1.3.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "prettier-eslint": "^8.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "nodemon": "^1.19.1",
    "pkg": "^4.4.0"
  }
}
