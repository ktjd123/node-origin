{
  "scripts": {
    "d": "nodemon -w package.json -w src --exec babel-node src/index.js",
    "build": "rm -rf build && babel src -d build && cp package.json ./build",
    "p": "pkg build/index.js --out-path build/executable",
    "start": "node ./build/index.js",
    "pm2": "pm2 start build/index.js -i 0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "colors": "^1.3.3",
    "command-line-args": "^5.0.2",
    "command-line-usage": "^5.0.5",
    "mongoose": "^5.4.11",
    "readline-sync": "^1.4.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "nodemon": "^1.18.10",
    "pkg": "^4.3.7"
  }
}
